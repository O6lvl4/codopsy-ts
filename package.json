{
  "name": "codopsy-ts",
  "version": "1.1.0",
  "description": "AST-level code quality analyzer for TypeScript & JavaScript. Cyclomatic & cognitive complexity, 13 lint rules, JSON/HTML/SARIF output.",
  "type": "module",
  "main": "dist/api.js",
  "types": "./dist/api.d.ts",
  "bin": {
    "codopsy-ts": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./cli": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.ja.md",
    "LICENSE",
    "action.yml"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "code-quality",
    "static-analysis",
    "cyclomatic-complexity",
    "cognitive-complexity",
    "quality-score",
    "linter",
    "typescript",
    "sarif",
    "github-action",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/O6lvl4/codopsy-ts"
  },
  "homepage": "https://github.com/O6lvl4/codopsy-ts#readme",
  "bugs": {
    "url": "https://github.com/O6lvl4/codopsy-ts/issues"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "glob": "^13.0.3",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18"
  }
}
